; ModuleID = 'kolga'
source_filename = "kolga"

define float @if1() {
if1:
}

define float @if2() {
if2:
}

define float @if3() {
if3:
}

define float @main() {
main:
  br i1 true, label %then, label %elifcond0

then:                                             ; preds = %main
  %0 = call float @if1()
  br label %merge

el:                                               ; No predecessors!

elifcond0:                                        ; preds = %main
  br i1 true, label %elifblck0, label %merge

elifblck0:                                        ; preds = %elifcond0
  %1 = call float @if2()
  br label %merge

merge:                                            ; preds = %elifblck0, %elifcond0, %then
  %phi = phi float [ %0, %then ], [ %1, %elifblck0 ]
  %2 = call float @if3()
}
